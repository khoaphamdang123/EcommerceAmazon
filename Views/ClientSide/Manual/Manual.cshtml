    <script>
        document.addEventListener("DOMContentLoaded",()=>{
         
         function checkMobileScreen()
         {
           return /Mobi|Android|iPhone|iPad|iPod/i.test(navigator.userAgent);
         }

        function downloadLink(url,filename)
        {
            fetch(url).then(response=>response.blob()).then(blob=>{
               const blobUrl=URL.createObjectURL(blob);
               
               const a=document.createElement("a");
                
                a.href=blobUrl;
                
                a.download=filename;
                
                document.body.appendChild(a);
                
                a.click();
                
                document.body.removeChild(a);

                URL.revokeObjectURL(blobUrl);                
            }).catch((error)=>
            {
                alert(error);
            }); 
        }

        var pdf_link=document.getElementById("pdf-download-detail");

        var pdf_temp_link=document.getElementById("pdf-download");

        pdf_temp_link.addEventListener("click",(e)=>
        {
            if(checkMobileScreen())
            {   
                e.preventDefault();
                const url=pdf_temp_link.href.replace('http','https');
                const filename=url.split('/').pop();
                downloadLink(url,filename);
            }
        });

            pdf_link.addEventListener("click",(e)=>
            {
                if(checkMobileScreen())
                {   
                    e.preventDefault();
                    const url=pdf_link.href.replace('http','https');
                    const filename=url.split('/').pop();
                    downloadLink(url,filename);
                }
            });
        });
    </script>


