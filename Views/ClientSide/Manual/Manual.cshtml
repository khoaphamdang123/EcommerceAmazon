@model List<Manual>
@using Newtonsoft.Json;
@{
    Layout = "_UserLayout";
    var avatar="";
    string product_name="";
    var video_list=ViewBag.videos as List<Video>;
    string pdf_link="";
    string web_link="";
    string pdf_modifield_date="";
    string web_modifield_date="";
    if(Model.Count>0)
    {
        avatar=@Model[0]?.Product.Frontavatar.Substring(Model[0].Product.Frontavatar.IndexOf("wwwroot")+("wwwroot".Length+1)).Replace("\\","/");

        product_name=Model[0].Product.ProductName;
        
    }
   Manual english_model=Model.Where(x=>(x.Language=="English" || x.Language.Contains("Vietnamese")) && x.ProductId==Model[0].ProductId).FirstOrDefault();

   if(english_model!=null)
   {
  
  string[] link_details=english_model.ManualLink.Split(',');
  
  if(link_details.Length>1)
  {
    pdf_link=link_details[0];
    
    web_link=link_details[1];

    pdf_modifield_date=english_model.UpdatedDate;

    web_modifield_date=english_model.UpdatedDate;

  }
  else if(link_details.Length==1)
  {
    if(link_details[0].Contains("pdf"))
    {
        pdf_link=link_details[0];
        pdf_modifield_date=english_model.UpdatedDate;

    }
    else
    {
        web_link=link_details[0];
        web_modifield_date=english_model.UpdatedDate;
  }
   }
   }
}
