        phone1=phones[0];
        phone2=phones[1];
       }
       else if(phones.Length==1)
       {
        phone1=phones[0];
       }
       string company_name_value=company_name[0];
       
       string address1=company_name[1];
        
       var cart = ViewBag.cart as List<CartModel>;
     
        Console.WriteLine("Current category list size is:"+categories.Count);

        Console.WriteLine("Current view controller is:"+view_action);
    }

