        string username="";
        string email="";
        string password = "";
        string avatar="";
       string logo_path="";

       string logo_path_temp="7e6df1cf-4caa-48a1-b115-66c650c5e069_channels4_profile.jpg";
    
        
      if(!string.IsNullOrEmpty(Environment.GetEnvironmentVariable("Logo")))
      {
      string logo=Environment.GetEnvironmentVariable("Logo");
       string logo_extension=logo.Substring(logo.IndexOf("wwwroot")+("wwwroot".Length+1));
       if(Environment.GetEnvironmentVariable("ASPNETCORE_ENVIRONMENT")=="Development")
       {
        logo_path="http://localhost:5160/"+logo_extension;
       }
       else
       {
    logo_path="https://thanhquang-gnss.com/"+logo_extension;
       }
       
    @* logo_path=logo_extension; *@

       Console.WriteLine("Current logo admin path is:"+logo_path);
       }
       
       int total_value=0;
       
       ChangePassword change_pass= null;
       
       if(User.Identity.IsAuthenticated && User.IsInRole("User"))
       {
        username=this.ViewContext.HttpContext.Session.GetString("Username");

        email=this.ViewContext.HttpContext.Session.GetString("Email");
        
        password = this.ViewContext.HttpContext.Session.GetString("Password");
        
        avatar = this.ViewContext.HttpContext.Session.GetString("Avatar");        
        
        change_pass=new ChangePassword(email,password,"");
       }

    var categories = ViewBag.Categories as List<Category>;
    
    var company = ViewBag.Company as ApplicationUser;
    
    string phone1="";
    
    string phone2="";
    
    string[] company_name=company.Address1.Split('!');

    string address2=company.Address2;
    
    string[] phones= company.PhoneNumber.Split(',');
       
    string[] email_list=company.Email.Split('#');
    
    string extra_info=email_list[1];
    
    string bank_name="";
    
    string account_name="";

    string facebook="";
    
    string zalo="";
    
    string youtube="";
    
    string instagram="";

    if(!string.IsNullOrEmpty(extra_info))
    {
      string[] info_values=extra_info.Split('\n');

      foreach(var info in info_values)
      {
        if(info.Contains("bank_name"))
        {
          bank_name=info.Split('~')[1].Trim();
          Environment.SetEnvironmentVariable("bank",bank_name);
        }
        else if(info.Contains("account_name"))
        {
          account_name=info.Split('~')[1].Trim();
        }
        else if(info.Contains("facebook"))
        {
          facebook=info.Split('~')[1].Trim();
        }
        else if(info.Contains("zalo"))
        {
          zalo=info.Split('~')[1].Trim();
        }
        else if(info.Contains("youtube"))
        {
          youtube=info.Split('~')[1].Trim();
        }
        else if(info.Contains("instagram"))
        {
          instagram=info.Split('~')[1].Trim();
        }
        else if(info.Contains("qr_code"))
        {
            Environment.SetEnvironmentVariable("QR_CODE",info.Split('~')[1].Trim());
        }
      }
    }

    
       
       
       if(phones.Length>1)
       {
        phone1=phones[0];
        phone2=phones[1];
       }
       else if(phones.Length==1)
       {
        phone1=phones[0];
       }
       string company_name_value=company_name[0];
       
       string address1=company_name[1];
        
        var cart = ViewBag.cart as List<CartModel>;
     
        Console.WriteLine("Current category list size is:"+categories.Count);

        Console.WriteLine("Current view controller is:"+view_action);
    }

