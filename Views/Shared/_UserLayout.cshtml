   <script type="text/javascript">
   
   document.addEventListener("DOMContentLoaded",()=>{
    const header = document.querySelector('.header');
    
    const main_body=document.getElementById('main-container');
    
    const headerHeight = header.offsetHeight;
    
    window.addEventListener('scroll',()=>{
        if(window.scrollY > (headerHeight+5)){
            header.classList.add('header-container');
            header.classList.add('fixed');
            main_body.classList.add('main-container');
        }else{
            header.classList.remove('header-container');
            header.classList.remove('fixed');
            main_body.classList.remove('main-container');
        }
    });
   });
   </script>

